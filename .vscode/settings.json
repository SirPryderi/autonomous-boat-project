{
  "cSpell.words": [
    "Adafruit",
    "atmelavr",
    "espressif",
    "OLED",
    "sysenv"
  ],
  "C_Cpp.clang_format_style": "file",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "ms-vscode.cpptools",
  "editor.tabSize": 2,
  "files.associations": {
    "*.ino": "cpp",
    "array": "cpp",
    "deque": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "string_view": "cpp",
    "memory": "cpp",
    "random": "cpp",
    "initializer_list": "cpp"
  }
}
